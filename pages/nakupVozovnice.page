        function showToaster(type) {
        	debugger;
        	if(type === 'insert') {
        		toastr.success('Vozovnica je bila shranjena!');
        	} else if(type === 'delete') {
        		toastr.error('Vozovnica je bila izbrisana!');
        	} else if(type === 'Placilo') {
        		toastr.success('Plačilo je bilo uspešno izvedeno!');
        	}
        }

        function checkPlacilo() {
        	debugger;
        	if(typeof $('input[name="options"]:checked').val() != 'undefined' && $('input[name="options"]:checked').val() != '') {
    		 	setStatus();
    		 	return;
        	}
        	$('#izbiraPlacilaError').removeClass('slds-hide');
        	return;
        }
        

        window.onbeforeunload = function() {
		  return "Podatki bodo izgubljeni, želite vseeno zapustiti stran?";
